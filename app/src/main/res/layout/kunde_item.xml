<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/kundeItemTextView_kundeNr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:padding="5dp"
        android:text="tittel"
        android:textAllCaps="true"
        android:textAppearance="@android:style/TextAppearance.Material.Large" />

    <TextView
        android:id="@+id/kundeItemTextView_fornavn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@id/kundeItemTextView_kundeNr"
        android:padding="5dp"
        android:text="pris"
        android:textAppearance="@android:style/TextAppearance.Material.Medium" />

    <TextView
        android:id="@+id/kundeItemTextView_etternavn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@id/kundeItemTextView_fornavn"
        android:padding="5dp"
        android:text="sjanger"
        android:textAppearance="@android:style/TextAppearance.Material.Medium" />

    <TextView
        android:id="@+id/kundeItemTextView_adresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/kundeItemTextView_etternavn"
        android:padding="5dp"
        android:text="tid"
        android:textAppearance="@android:style/TextAppearance.Material.Small" />

    <TextView
        android:id="@+id/kundeItemTextView_postNr"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/kundeItemTextView_adresse"
        android:padding="5dp"
        android:text="tid"
        android:textAppearance="@android:style/TextAppearance.Material.Small" />

</RelativeLayout>